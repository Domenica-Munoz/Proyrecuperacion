<?xml version="1.0" encoding="UTF-8"?>
<faces-config
    xmlns="http://xmlns.jcp.org/xml/ns/javaee"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_3.xsd"
    version="2.3">
    
    <navigation-rule>
	
		<from-view-id>/loguin.xhtml</from-view-id>
		
		<navigation-case>
			<from-outcome>inicio</from-outcome>
			<to-view-id>/Comentarios.xhtml</to-view-id>
		</navigation-case>
		
		<navigation-case>
			<from-outcome>crear</from-outcome>
			<to-view-id>/crearCliente.xhtml</to-view-id>
		</navigation-case>
		
		
	</navigation-rule>
	
	<navigation-rule>
	
		<from-view-id>/crearCliente.xhtml</from-view-id>
		
		<navigation-case>
			<from-outcome>Inicio</from-outcome>
			<to-view-id>/loguin.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>
	
	<navigation-rule>
	
		<from-view-id>/Comentarios.xhtml</from-view-id>
		
		<navigation-case>
			<from-outcome>Inicio</from-outcome>
			<to-view-id>/loguin.xhtml</to-view-id>
		</navigation-case>
		
	</navigation-rule>
	

</faces-config>
